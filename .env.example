# Google API Key (required for Gemini LLM)
GOOGLE_API_KEY=your-google-api-key-here

# Keycloak Admin (used by docker-compose)
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=welcome

# NPL Engine
NPL_ENGINE_URL=http://localhost:12000

# Keycloak Base URL
NPL_KEYCLOAK_URL=http://localhost:11000

# Default realm/user (used by some scripts)
# Note: Agents use 'purchasing' or 'supplier' realms
NPL_KEYCLOAK_REALM=purchasing
NPL_KEYCLOAK_CLIENT_ID=purchasing
NPL_USERNAME=purchasing_agent

# Password for all test users (provisioned by Keycloak)
SEED_TEST_USERS_PASSWORD=Welcome123

# Optional: For paid Gemini billing
# GOOGLE_CLOUD_PROJECT=your-project-id
